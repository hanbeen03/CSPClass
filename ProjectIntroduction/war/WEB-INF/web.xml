<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ProjectIntroduction</servlet-name>
		<servlet-class>kr.ac.shinhan.ProjectIntroductionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProjectIntroduction</servlet-name>
		<url-pattern>/projectintroduction</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RetrieveMember</servlet-name>
		<servlet-class>kr.ac.shinhan.RetrieveMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RetrieveMember</servlet-name>
		<url-pattern>/retrievemember</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ChangeServlet</servlet-name>
		<servlet-class>kr.ac.shinhan.ChangeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangeServlet</servlet-name>
		<url-pattern>/changeservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>RemoveServlet</servlet-name>
		<servlet-class>kr.ac.shinhan.RemoveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RemoveServlet</servlet-name>
		<url-pattern>/removeservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ApproveServlet</servlet-name>
		<servlet-class>kr.ac.shinhan.ApproveServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ApproveServlet</servlet-name>
		<url-pattern>/approveservlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>kr.ac.shinhan.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/loginservlet</url-pattern>
	</servlet-mapping>
	
	<servlet> 
		<servlet-name>SignUp</servlet-name> 
		<servlet-class>kr.ac.shinhan.SignUpServlet</servlet-class> 
	</servlet> 
	<servlet-mapping> 
		<servlet-name>SignUp</servlet-name> 
		<url-pattern>/signup</url-pattern> 
	</servlet-mapping> 
	
	<servlet> 
		<servlet-name>Entry</servlet-name> 
		<servlet-class>kr.ac.shinhan.EntryServlet</servlet-class> 
	</servlet> 
	<servlet-mapping> 
		<servlet-name>Entry</servlet-name> 
		<url-pattern>/entry</url-pattern> 
	</servlet-mapping>
	
	<servlet> 
		<servlet-name>LogOut</servlet-name> 
		<servlet-class>kr.ac.shinhan.LogOutServlet</servlet-class> 
	</servlet> 
	<servlet-mapping> 
		<servlet-name>LogOut</servlet-name> 
		<url-pattern>/logout</url-pattern> 
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>entry</welcome-file>
	</welcome-file-list>
		
</web-app>
